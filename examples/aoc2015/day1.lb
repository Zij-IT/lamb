def part_one := rec fn(xs) -> case xs {
  ['(', rest @ ..] -> 1 + part_one(rest),
  [')', rest @ ..] -> -1 + part_one(rest),
  [] -> 0,
};

def part_two := fn(xs) -> {
  inner := rec fn(xs, n, sum) -> case sum {
    -1 -> n,
    _  -> case xs {
      ['(', rest @ ..] -> inner(rest, n + 1, sum + 1),
      [')', rest @ ..] -> inner(rest, n + 1, sum - 1),
    },
  };

  inner(xs, 0, 0)
};

def main := fn() -> {
  print("Part One:: ");
  input $> part_one .> println;

  print("Part Two:: ");
  input $> part_two .> println;
};
