def main := fn() -> {
  inc := fn(x) -> x + 1;

  dec := fn(x) -> x - 1;

  dub := fn(x) -> x * 2;

  fib := rec fn(x) -> if x <= 1 { x } else { fib(x - 1) + fib(x - 2) };

  print("Should print 17:: ");
  println <. dec <. dub <. inc <$ 8;

  print("Should print 5:: ");
  2 $> inc .> dub .> dec .> println;
};
