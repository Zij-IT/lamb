def insertionSort
  : fn[t](list[t]) -> list[t]
  = fn(xs) -> {

    let insert := rec fn(ys, y) -> case ys {
      [head, rest @ ..] -> if y > head { [head] + insert(rest, y) } else { [y] + ys },
      [] -> [y],
    };

    let impl := rec fn(xs, acc) -> case xs {
      [a, rest @ ..] -> impl(rest, insert(acc, a)),
      [] -> acc,
    };

    impl(xs, [])
  };

def main
  : fn() -> nil
  = fn() -> {
    [1, 5, 2, 3, 7] $> insertionSort .> println;
  };

def _start: nil = main();
