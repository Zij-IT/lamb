-- * has higher precedence than +.
assert_eq(2 + 3 * 4, 14);

-- * has higher precedence than -.
assert_eq(20 - 3 * 4, 8);

-- / has higher precedence than +.
assert_eq(2 + 6 / 3, 4);

-- / has higher precedence than -.
assert_eq(2 - 6 / 3, 0);

-- < has higher precedence than =.
assert_eq(false = 2 < 1, true);

-- > has higher precedence than =.
assert_eq(false = 1 > 2, true);

-- <= has higher precedence than =.
assert_eq(false = 2 <= 1, true);

-- >= has higher precedence than =.
assert_eq(false = 1 >= 2, true);

-- 1 - 1 is not space-sensitive.
assert_eq(1 - 1, 0);
assert_eq(1 -1, 0);
assert_eq(1- 1, 0);
assert_eq(1-1, 0);

-- Using () for grouping.
assert_eq(2 * (6 - (2 + 2)), 4);
